<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />

    <title><%- title %></title>

    <!-- Google font -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons+Outlined"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Azeret+Mono:wght@300;400;500;700&family=Lato:wght@100;300;400;700&family=Noto+Sans+TC:wght@100;400;500;700&family=Rubik:wght@300;400;500;600;700;800;900&display=swap"
      rel="stylesheet"
    />
    <!-- FontAwesome -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
    <!-- Swiper -->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.css"
    />
    <!-- vanillajs-datepicker -->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/vanillajs-datepicker@1.3.1/dist/css/datepicker.min.css"
    />
    <!-- AOS -->
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet" />
    <!-- CSS -->
    <link rel="stylesheet" href="./assets/style/all.css" />
  </head>

  <body>
    <!-- header -->
    <header
      class="bg-white relative z-100 drop-shadow-md before:absolute before:bottom-0 before:w-full before:h-[1px] before:navbar-gradient"
    >
      <div class="container flex justify-between py-6 relative">
        <!-- logo -->
        <h1>
          <a
            href="index.html"
            class="w-6 h-6 block bg-[url('../images/logo-square.png')] bg-contain bg-no-repeat indent-[101%] overflow-hidden whitespace-nowrap lg:bg-[url('../images/logo_horizontal.png')] md:w-[175px] md:h-[42px] hover:opacity-50"
            >Coding Home</a
          >
        </h1>
        <!-- desktop nav -->
        <nav class="hidden justify-end items-center md:flex">
          <a href="#about" class="px-6 py-2 hover:opacity-50">About</a>
          <a href="#workExp" class="px-6 py-2 hover:opacity-50">Experience</a>
          <a href="#projects" class="px-6 py-2 hover:opacity-50">Portfolio</a>
          <a href="#contact" class="px-6 py-2 hover:opacity-50">Contact</a>
          <a
            href="https://lihi3.cc/tydM7"
            target="_blank"
            class="btn btn-primary px-6 py-2"
            >Resume</a
          >
        </nav>
        <!-- mobile nav -->
        <a
          id="navMobileBtn"
          class="material-icons w-6 h-6 text-primary md:hidden"
          href="#"
          >drag_handle</a
        >
        <div
          id="navMobileMenu"
          class="absolute top-[72px] left-0 w-full z-10 bg-primary text-white shadow-lg hidden md:hidden"
        >
          <nav class="text-center divide-y">
            <a href="#about" class="text-md py-4">About</a>
            <a href="#workExp" class="text-md py-4">Experience</a>
            <a href="#projects" class="text-md py-4">Projects</a>
            <a href="#contact" class="text-md py-4">Contact</a>
            <a
              href="https://lihi3.cc/tydM7"
              target="_blank"
              class="text-md py-4"
              >Resume</a
            >
          </nav>
        </div>
      </div>
    </header>

    <%- contents %>

    <footer class="bg-[#1A1A1A] py-3 md:py-6">
      <p
        class="text-center text-xs text-[#909090] font-azeret md:text-base md:leading-[19px]"
      >
        Â© Designed & Developed by YACHU
      </p>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.js"></script>
    <script src="./assets/js/vendors.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vanillajs-datepicker@1.3.1/dist/js/datepicker-full.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vanillajs-datepicker@1.3.1/dist/js/locales/zh-TW.js"></script>
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script src="./assets/js/all.js"></script>
  </body>
</html>
